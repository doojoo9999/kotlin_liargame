# .env.staging (스테이징 환경)
VITE_API_BASE_URL=https://staging-api.yourdomain.com
VITE_WEBSOCKET_URL=wss://staging-api.yourdomain.com
VITE_FRONTEND_PORT=5173
VITE_USE_DUMMY_WEBSOCKET=false
VITE_USE_DUMMY_DATA=false
VITE_ENVIRONMENT=staging

# 게임 설정
VITE_MIN_PLAYERS=3
VITE_MAX_PLAYERS=15
VITE_MIN_ROUNDS=1
VITE_MAX_ROUNDS=10
VITE_DEFAULT_ROUNDS=3
VITE_CHAT_MESSAGE_LIMIT=500

# 디버그 설정
VITE_ENABLE_DEBUG_LOGS=true

# 타임아웃 설정 (밀리초)
VITE_API_TIMEOUT=10000
VITE_WS_TIMEOUT=5000
VITE_GAME_PHASE_TIMEOUT=60000